{
    "name"        : "i55WebManager",
    "type"        : "library",
    "description" : "Manage your i3 applications",
    "keywords"    : ["framework"],
    "homepage"    : "",
    "license"     : "MIT",
    "authors"     : [
        {
            "name"  : "Raphaël Khaïat",
            "email" : "raphael@khaiat.org"
        }
    ],
    "require": {
        "php"                   : ">=5.3.3",
        "silex/silex"           : "dev-master",
        "twig/twig"             : "1.*",
        "monolog/monolog"       : "1.0.*",
        "symfony/form"          : "2.1.*",
        "symfony/translation"   : "2.1.*",
        "symfony/twig-bridge"   : "2.1.*",
        "symfony/validator"     : "2.1.*",
        "symfony/yaml"          : "2.1.*",
        "symfony/config"        : "2.1.*",
        "symfony/console"       : "2.1.*",
        "kriswallsmith/assetic" : "1.0.*",
        "twitter/bootstrap"     : "v2.2.2",
        "mheap/silex-assetic"   : "dev-master"
    },
    "require-dev": {
        "symfony/dom-crawler"   : "2.1.*",
        "symfony/css-selector"  : "2.1.*",
        "symfony/browser-kit"   : "2.1.*"
    },
    "minimum-stability" : "dev",
    "scripts": {
        "post-install-cmd": "SKE\\Composer\\Script::postInstall"
    },
    "autoload": {
        "psr-0": {
            "SKE": "src/",
            "B55": "src/"
        }
    }
}
